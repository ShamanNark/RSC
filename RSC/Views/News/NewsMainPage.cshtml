    .top-all-news .btn-transparent {
        margin-left: auto;
        margin-right: auto;
        text-align: center;
        font-weight: bold;
        border: solid #ffffff 2px;
        z-index: 10;
        position: absolute;
        width: 80%;
        top: 70%;
        left: 9%;
    }

    .news-main-block{
        margin-top: 20px;
        padding-left:0px;
        padding-right: 0px;
    }

    .news-main-block-left{
