    }


    .big-news-block-dav {
        position: absolute;
        top: 80%;
        padding-left: 10%;
        padding-right: 20%;
        width: 100%;
        color: snow;
        height: 60px;
        font-weight: bold;
        text-transform: uppercase;
        font-size: 20px;
    }
        .big-news-block h4 {
            
        }

    .one-mini-news {
        padding-left:0px;
        padding-right: 0px;
        min-height: 100px;
    }

    .one-mini-news-text p {
        text-transform: uppercase;
        color: black;
        font-weight: bold;
        word-wrap: break-word;
    } 
    .one-mini-news-text {
        padding-right: 20px;
    }


